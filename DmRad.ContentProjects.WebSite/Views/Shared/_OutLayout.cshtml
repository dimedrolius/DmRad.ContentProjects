<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Styles/out.css")
    @Styles.Render("~/Styles/bootstrap.css")

    @Scripts.Render("~/Scripts/jquery-2.1.3.min.js")
    @Scripts.Render("~/Scripts/jquery-ui.min.js")
    @Scripts.Render("~/Scripts/jquery-migrate-1.2.1.min.js")
    @Scripts.Render("~/Scripts/bootstrap.min.js")
    @Scripts.Render("~/Scripts/script.js")
</head>
<body>
    @Html.Partial("_CustomDialogs")

    <div class="container" style="width: 900px;">
        <div class="jumbotron alert-info">
            <div class="row">
                <div class="col-sm-3">
                    <a href="@Url.Action("Index", "Home")"><img src="/Images/mintrans.gif" style="width: 200px; margin-left: -10px;" alt="" /></a>
                </div>
                <div class="col-sm-9" style="text-align: center; margin-top: 50px;">
                    <h2>Министерство транспорта МО</h2>
                    <h3>Единая транспортная карта</h3>
                </div>
            </div>
        </div>

        @RenderBody()
    </div>
</body>
</html>
